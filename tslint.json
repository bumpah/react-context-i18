{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rules": {
    "semicolon": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-name": false,
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false
  }
}